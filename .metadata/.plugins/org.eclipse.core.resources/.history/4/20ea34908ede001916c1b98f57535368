package com.bridgelabz.login;

import java.sql.SQLException;

import org.apache.catalina.User;

import com.mysql.jdbc.Connection;
import com.mysql.jdbc.PreparedStatement;

public class DatabaseDAOImplementation implements DatabaseDAO 
{
	static Connection connection;
	PreparedStatement preparedSt;
	
	@Override
	public int insertUserIntoDatabase(User user) {
		
		try 
		{
			connection = MyConnection.getConnection();
			preparedSt = (PreparedStatement) connection.prepareStatement("INSERT INTO Login_data VALUES(?,?,?)");
			preparedSt.setString(1, user.getName());
			preparedSt.setString(2, user.getE);
			
		}
		catch (SQLException e) 
		{		
			e.printStackTrace();
		}
		return 0;
	}

	@Override
	public User getFromDatabase(String username, String pass) {
		
		return null;
	}

	

}
